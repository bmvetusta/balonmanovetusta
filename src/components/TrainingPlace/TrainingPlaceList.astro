---
import trainingPlaces from 'src/content/trainings/trainings.json' with { type: 'json' };
import TrainingPlaceItem from './TrainingPlaceItem.astro';
---

<div>
  {trainingPlaces?.map((place) => <TrainingPlaceItem {...place} />)}
</div>

<style>
  div {
    display: flex;
    flex-direction: column;
    gap: 0.5rem;
  }
</style>
